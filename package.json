{
  "name": "donq",
  "version": "0.0.1",
  "description": "DonQ cli",
  "keywords": [
    "oclif"
  ],
  "homepage": "https://github.com/donq/donq_cli",
  "bugs": "https://github.com/donq/donq_cli/issues",
  "repository": "npm-package/donq",
  "license": "MIT",
  "author": "Federico Serafini",
  "exports": "./lib/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "donq": "./bin/ru.js"
  },
  "files": [
    "/bin",
    "/dist",
    "/oclif.manifest.json"
  ],
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts",
    "prepack": "pnpm run build && oclif manifest && oclif readme",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepare": "pnpm run build",
    "test": "mocha --forbid-only \"test/**/*.test.ts\"",
    "posttest": "pnpm run lint",
    "version": "oclif readme && git add README.md",
    "plop": "plop",
    "handlebars": "handlebars"
  },
  "oclif": {
    "bin": "donq",
    "commands": "./dist/commands",
    "dirname": "donq",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " "
    }
  },
  "dependencies": {
    "@aws-sdk/client-ecs": "^3.563.0",
    "@aws-sdk/credential-providers": "^3.563.0",
    "@oclif/core": "^3",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "handlebars": "^4.7.8",
    "inquirer": "^9.2.19",
    "plop": "^4.0.1"
  },
  "devDependencies": {
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^3",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/chai": "^4",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "chai": "^4",
    "eslint": "^8",
    "eslint-config-oclif": "^5",
    "eslint-config-oclif-typescript": "^3",
    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.8.5",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  }
}
